{
  "version": 2,
  "github": {
    "silent": true
  },
  "buildCommand": "cd full-kit && pnpm install && pnpm build",
  "installCommand": "pnpm install --no-frozen-lockfile",
  "outputDirectory": "full-kit/.next",
  "framework": "nextjs",
  "regions": ["iad1"],
  "functions": {
    "full-kit/src/app/api/**/*.{js,ts}": {
      "maxDuration": 30
    }
  },
  "redirects": [
    {
      "source": "/",
      "destination": "/en/dashboards/analytics",
      "permanent": false
    }
  ]
}
