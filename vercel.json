{
  "github": {
    "silent": true
  },
  "buildCommand": "pnpm build",
  "installCommand": "pnpm install",
  "outputDirectory": "full-kit/.next",
  "framework": "nextjs",
  "regions": ["iad1"],
  "rewrites": [
    {
      "source": "/starter/(.*)",
      "destination": "/starter-kit/$1"
    }
  ]
}
