{
  "name": "gsdashboard",
  "version": "2",
  "framework": "nextjs",
  "buildCommand": "cd full-kit && pnpm install && pnpm exec prisma generate && pnpm build",
  "installCommand": "cd full-kit && pnpm install",
  "outputDirectory": "full-kit/.next",
  "regions": ["iad1"],
  "env": {
    "BASE_URL": "https://gsdashboard-five.vercel.app",
    "API_URL": "https://gsdashboard-five.vercel.app/api",
    "NEXTAUTH_URL": "https://gsdashboard-five.vercel.app",
    "NEXTAUTH_SECRET": "gsdashboard_next_auth_secret_key_for_production_2024_secure_random_32chars",
    "DATABASE_URL": "file:./dev.db",
    "HOME_PATHNAME": "/dashboards/analytics",
    "NEXT_PUBLIC_HOME_PATHNAME": "/dashboards/analytics"
  },
  "build": {
    "env": {
      "SKIP_ENV_VALIDATION": "1"
    }
  },
  "functions": {
    "app/api/**/*.ts": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/starter/(.*)",
      "destination": "/starter-kit/$1"
    }
  ],
  "redirects": [
    {
      "source": "/",
      "destination": "/en/dashboards/analytics",
      "permanent": false
    }
  ]
}
